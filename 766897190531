// 766897190531
#include<stdio.h>
int max_element(int arr[], int m) {
    int i = 1, max = arr[0];
    while (i < m) {
        if (arr[i] > max) {
            max = arr[i];
        }
        i++;
    }
    return max;
}
int main() {
    int n;
    printf("Enter the array size: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter the elements of array: \n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int a = max_element(arr, n);
    printf("Maximum element of array is: %d", a);
    return 0;
}
