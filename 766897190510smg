// 766897190510smg
#include <stdio.h>
#define SIZE 1000
// That code only read positive numbers ...
[int main() 
{
    int arr[SIZE] = {0};         // Initialize array to store the frequency of numbers
    int m, count = 0;
    printf("Enter integers (enter -1 to stop):\n");
    while (1) 
    {
        scanf("%d", &m);
        if (m == -1)     // Stop reading the further inputs 
            break;
        if (m >=0 && m < SIZE)     // check number is within the range of array size
            arr[m]++;
        else
            printf("Invalid input: %d\n", m);
    }
     printf("Frequency of number :\n");
     for (int i = 0; i < SIZE; i++) 
     {
        if (arr[i] != 0) 
        {
            printf("%d occurs is %d times\n", i, arr[i]);
            count++;
        }
    }
     if(count==0)
     {
        printf("No numbers entered.\n");
     }
    return 0;
}
