#include <stdio.h>
// Function to check number is prime or not.. 
int is_prime(int n) 
{
    if (n <= 1) 
    {
        return 0; 
    }
    for (int i = 2; i*i <= n; i++) 
    {
        if (n % i == 0) 
        {
            return 0 ; 
        }
    }
    return 1;       //when number is prime 
}
int main() 
{
    int n;
    printf("Enter the number: ");
    scanf("%d", &n);
    if (is_prime(n)) 
    {
        printf("%d is a prime number.\n", n);
    }
    else 
    {
        printf("%d is not a prime number.\n", n);
    }
    return 0;
}
