// 766897190547cl256
#include <stdio.h>
void swap(int * a, int * b) 
{    
    int temp;
    temp=*b;
    *b=*a;
    *a=temp;
}
int main () 
{
    int a, b;
    int *ptr1, *ptr2;
    ptr1=&a;
    ptr2=&b;
    printf("Enter the two numbers:");
    scanf("%d\n%d",&a,&b);
    printf("Numbers before swap :a=%d,b=%d\n",a,b);
    swap(ptr1, ptr2);
    printf("Number after swap : a=%d,b=%d\n",a,b);
    return 0;
}
